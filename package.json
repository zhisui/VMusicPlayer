{
  "name": "vmusicplayer",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=14",
    "npm": ">=7.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production pnpm run build",
    "compile": "electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "pretest": "pnpm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "pnpm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer",
    "pretypecheck-renderer": "dts-cb -i packages/preload/src/**/*.ts -o packages/preload/exposedInMainWorld.d.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.18.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@iconify/vue": "^3.1.1",
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
    "@quitsmx/eslint-config": "^0.5.5",
    "@types/crypto-js": "^4.1.0",
    "@types/electron-devtools-installer": "2.2.1",
    "@types/eslint": "^7.28.2",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^17.0.8",
    "@types/nprogress": "^0.2.0",
    "@types/prettier": "^2.4.1",
    "@types/qrcode": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vitejs/plugin-vue": "2.0.1",
    "commitizen": "^4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dts-for-context-bridge": "0.7.1",
    "electron": "16.0.5",
    "electron-builder": "22.14.5",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.5.0",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettierx": "^0.18.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "eslint-plugin-vue": "8.2.0",
    "nano-staged": "0.5.0",
    "playwright": "1.17.1",
    "prettier": "github:aMarCruz/prettier",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "simple-git-hooks": "2.7.0",
    "typescript": "4.5.4",
    "unplugin-icons": "^0.13.0",
    "unplugin-vue-components": "^0.17.11",
    "vite": "2.7.7",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "0.30.1"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "dexie": "^3.2.0",
    "electron-updater": "4.6.5",
    "howler": "^2.2.3",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.10",
    "qrcode": "^1.5.0",
    "vue": "3.2.26",
    "vue-clipboard2": "^0.3.3",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "4.0.12"
  }
}
